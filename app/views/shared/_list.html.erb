<div class="home-area-signed">
    <div class="list-container">
        <div class="list-caption">就緒！</div>
        <table class="list-table">
            <td id="list-head" colspan="4" style="border-spacing:10px;" border="1">
                <%= current_user.email %> 的工作空間
            </td>
            <% channels.each do |channel| %>
                <tr>
                    <td id="home-list">icons</td>
                    <td width="450px" height="60px" bgcolor="#fff" >
                        <span style="font-size:20px;" style="padding:10px">
                            <%= channel.space.name %>
                        </span>
                    </td>
                </tr>
            
                <td width="150px" bgcolor="#fff" style="padding:10px">
                    <span style="font-size:20px;">
                        <%= link_to "啟動STORK" , space_channel_path(channel.space.id ,channel.id) %>
                    </span>
                </td>
                <!-- <td style="display: none;">
                                    <span style="font-size:12px;"><%#= link_to "編輯", edit_page_path(s.id) %>／<%#= link_to "刪除", page_path(s.id),
                                method: 'delete',
                                "data-confirm": "確定嗎？" %></span>
                                </td> -->
                <% end %>
        </table>
        <table class="list-table">
            <tr id="list-head">
                <td id="list-spaces-start-stork">
                    <%= link_to "建立新空間" , stork_step1_path %>
                </td>
                <td>
                    <%= link_to "登出" , destroy_user_session_path, method: :delete %>
                </td>
            </tr>
        </table>
        <div>
            　
        </div>

    </div>
</div>